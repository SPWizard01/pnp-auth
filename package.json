{
  "name": "pnp-auth",
  "version": "0.0.4",
  "description": "Provides additional authentication options for @pnp/pnpjs (aka PnPjs) library",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "build": "npm run tslint && tsc -p .",
    "prepare": "rimraf -- lib && npm run build",
    "tslint": "tslint --project tsconfig.json",
    "precommit": "npm run tslint",
    "test": "mocha ./test/integration.spec.ts",
    "coverage": "nyc --report-dir coverage mocha ./test/integration.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SharePoint-NodeJS/pnp-auth.git"
  },
  "author": "",
  "contributors": [
    "Sergei Sergeev <sergeev.srg@gmail.com>",
    "Andrew Koltyakov <andrew.koltyakov@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SharePoint-NodeJS/pnp-auth/issues"
  },
  "homepage": "https://github.com/SharePoint-NodeJS/pnp-auth",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "peerDependencies": {
    "@pnp/common-commonjs": "2.x",
    "@pnp/logging-commonjs": "2.x",
    "@pnp/odata-commonjs": "2.x",
    "@pnp/sp-commonjs": "2.x"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node-fetch": "^2.1.2",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "node-sp-auth": "^2.5.9",
    "node-sp-auth-config": "^2.9.4"
  }
}
